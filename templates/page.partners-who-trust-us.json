    "advanced_content_8kMPyJ": {
      "type": "advanced-content",
      "blocks": {
        "image_p7PGp3": {
          "type": "image",
          "settings": {
            "image": "shopify://shop_images/EENA.jpg",
            "link": "https://eenaofficial.com/",
            "width": "50%",
            "alignment": "center"
          }
        },
        "liquid_yatqhq": {
          "type": "liquid",
          "settings": {
            "code": "<p dir=\"ltr\"><meta charset=\"utf-8\">Working with the Summer by Priyanka Gupta team has been a delight. Their designs are consistently beautiful with soft pastel tones and high-quality craftsmanship that feels timeless and thoughtful. But what truly sets them apart is their kindness, professionalism, and spirit of collaboration. We’re always excited to see what they create next!</p><p>EENA bags are available to shop at D 6, Defence Colony New Delhi</p>\n<p dir=\"ltr\"><br><strong>—Minakshi Goyal (Founder)</strong><br></p>",
            "width": "50%",
            "alignment": "center"
          }
        }
      },
      "block_order": [
        "image_p7PGp3",
        "liquid_yatqhq"
      ],
